		[     ]
		[ RRR ]
		[ RRR ]
		[ RRR ]
		[     ]
	]
	[
		[ RRR ]
		[R   R]
		[R S R]
		[R   R]
		[ RRR ]
	]
}

#structure:brownmushroomtop {
	origin: 3, 0, 3
